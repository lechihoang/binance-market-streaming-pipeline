    {
      "id": 7,
      "title": "Top Gainers",
      "type": "table",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/market/top-movers?direction=gainers&limit=10",
          "fields": [
            { "jsonPath": "$[*].symbol", "name": "Symbol" },
            { "jsonPath": "$[*].price", "name": "Price" },
            { "jsonPath": "$[*].change_percent", "name": "Change%" },
            { "jsonPath": "$[*].volume", "name": "Volume" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Price" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Change%" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "thresholds", "value": {
                "mode": "absolute",
                "steps": [
                  { "color": "red", "value": null },
                  { "color": "green", "value": 0 }
                ]
              }}
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Volume" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"],
          "fields": ""
        }
      }
    },
