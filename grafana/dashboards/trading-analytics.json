    {
      "id": 1,
      "title": "Total Trades (1h) - ${symbol}USDT",
      "type": "stat",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/klines/${symbol}USDT?interval=1m&start=$__from&end=$__to",
          "fields": [
            { "jsonPath": "$[*].trades_count", "name": "Trades" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["sum"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
