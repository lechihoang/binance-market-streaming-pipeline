    {
      "id": 8,
      "title": "Price Change (1h) - ${symbol}USDT",
      "type": "stat",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/market/realtime",
          "fields": [
            { "jsonPath": "$.tickers[*].symbol", "name": "Symbol" },
            { "jsonPath": "$.tickers[*].price_change_pct", "name": "Change %" }
          ]
        }
      ],
      "transformations": [
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "fieldName": "Symbol",
                "config": {
                  "id": "equal",
                  "options": { "value": "${symbol}USDT" }
                }
              }
            ],
            "type": "include",
            "match": "any"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 0 }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "/Change %/",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "text": {
          "valueSize": 38
        }
      }
    },

    {
      "id": 3,
      "title": "Whale Alerts",
      "type": "table",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/alerts/whale-alerts?limit=50",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].symbol", "name": "Symbol" },
            { "jsonPath": "$[*].side", "name": "Side" },
            { "jsonPath": "$[*].amount", "name": "Amount" },
            { "jsonPath": "$[*].price", "name": "Price" },
            { "jsonPath": "$[*].total_value", "name": "Total Value" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.width", "value": 180 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Symbol" },
            "properties": [
              { "id": "custom.width", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Side" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "custom.width", "value": 80 },
              { "id": "mappings", "value": [
                { "type": "value", "options": { "BUY": { "color": "green", "text": "BUY" } } },
                { "type": "value", "options": { "SELL": { "color": "red", "text": "SELL" } } }
              ]}
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Amount" },
            "properties": [
              { "id": "decimals", "value": 4 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Price" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Total Value" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 0 },
              { "id": "custom.displayMode", "value": "color-background" },
              { "id": "thresholds", "value": {
                "mode": "absolute",
                "steps": [
                  { "color": "transparent", "value": null },
                  { "color": "yellow", "value": 100000 },
                  { "color": "orange", "value": 500000 },
                  { "color": "red", "value": 1000000 }
                ]
              }}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"],
          "fields": ""
        },
        "sortBy": [
          { "displayName": "Time", "desc": true }
        ]
      }
    },

