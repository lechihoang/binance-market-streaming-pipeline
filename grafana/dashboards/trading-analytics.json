  "panels": [
    {
      "id": 1,
      "title": "Số giao dịch theo thời gian - ${symbol}USDT",
      "type": "barchart",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/trades-count?symbol=${symbol}USDT&interval=${interval}&limit=24",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].trades_count", "name": "Số giao dịch" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Số giao dịch",
            "axisPlacement": "auto",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineWidth": 1,
            "scaleDistribution": {
              "type": "linear"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "barRadius": 0,
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "orientation": "vertical",
        "showValue": "auto",
        "stacking": "none",
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "xTickLabelRotation": -45,
        "xTickLabelSpacing": 0
      }
    },

    {
      "id": 3,
      "title": "Whale Alerts",
      "type": "table",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/alerts/whale-alerts?limit=50",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].symbol", "name": "Symbol" },
            { "jsonPath": "$[*].side", "name": "Side" },
            { "jsonPath": "$[*].amount", "name": "Amount" },
            { "jsonPath": "$[*].price", "name": "Price" },
            { "jsonPath": "$[*].total_value", "name": "Total Value" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.width", "value": 180 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Symbol" },
            "properties": [
              { "id": "custom.width", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Side" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "custom.width", "value": 80 },
              { "id": "mappings", "value": [
                { "type": "value", "options": { "BUY": { "color": "green", "text": "BUY" } } },
                { "type": "value", "options": { "SELL": { "color": "red", "text": "SELL" } } }
              ]}
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Amount" },
            "properties": [
              { "id": "decimals", "value": 4 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Price" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Total Value" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 0 },
              { "id": "custom.displayMode", "value": "color-background" },
              { "id": "thresholds", "value": {
                "mode": "absolute",
                "steps": [
                  { "color": "transparent", "value": null },
                  { "color": "yellow", "value": 100000 },
                  { "color": "orange", "value": 500000 },
                  { "color": "red", "value": 1000000 }
                ]
              }}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"],
          "fields": ""
        },
        "sortBy": [
          { "displayName": "Time", "desc": true }
        ]
      }
    },

