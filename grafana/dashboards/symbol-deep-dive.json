  "panels": [
    {
      "id": 1,
      "title": "Price Chart - $symbol",
      "type": "timeseries",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/ohlc?symbol=${symbol}USDT&interval=${interval}",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].open", "name": "Open" },
            { "jsonPath": "$[*].high", "name": "High" },
            { "jsonPath": "$[*].low", "name": "Low" },
            { "jsonPath": "$[*].close", "name": "Close" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 2,
      "title": "RSI - $symbol",
      "type": "timeseries",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/indicators/${symbol}USDT?indicators=rsi&period=24h",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].rsi", "name": "RSI" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "RSI",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 70 },
              { "color": "blue", "value": 30 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": ["lastNotNull"],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      }
    },
    {
      "id": 3,
      "title": "MACD - $symbol",
      "type": "timeseries",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 10
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/indicators/${symbol}USDT?indicators=macd,macd_signal&period=24h",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].macd", "name": "MACD Line" },
            { "jsonPath": "$[*].macd_signal", "name": "Signal Line" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": true,
            "axisColorMode": "text",
            "axisLabel": "MACD",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 0,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Histogram" },
            "properties": [
              { "id": "custom.drawStyle", "value": "bars" },
              { "id": "custom.fillOpacity", "value": 50 }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["lastNotNull"],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 5,
      "title": "Technical Indicators (PostgreSQL) - $symbol",
      "type": "table",
      "datasource": {
        "type": "postgres",
        "uid": "postgresql"
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "postgres",
            "uid": "postgresql"
          },
          "rawSql": "SELECT timestamp AS \"Time\", rsi AS \"RSI\", macd AS \"MACD\", macd_signal AS \"Signal\", sma_20 AS \"SMA20\", ema_12 AS \"EMA12\", bb_upper AS \"BB Upper\", bb_lower AS \"BB Lower\" FROM indicators WHERE symbol = '${symbol}USDT' AND timestamp >= NOW() - INTERVAL '6 hours' ORDER BY timestamp DESC LIMIT 20",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "decimals": 2
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.width", "value": 180 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "RSI" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "thresholds", "value": {
                "mode": "absolute",
                "steps": [
                  { "color": "blue", "value": null },
                  { "color": "green", "value": 30 },
                  { "color": "red", "value": 70 }
                ]
              }}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"],
          "fields": ""
        }
      }
    },
    {
      "id": 4,
      "title": "Recent Trades - $symbol",
      "type": "table",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/market/trades/${symbol}USDT?limit=50",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].price", "name": "Price" },
            { "jsonPath": "$[*].quantity", "name": "Quantity" },
            { "jsonPath": "$[*].side", "name": "Side" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.width", "value": 200 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Price" },
            "properties": [
              { "id": "unit", "value": "currencyUSD" },
              { "id": "decimals", "value": 2 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Quantity" },
            "properties": [
              { "id": "decimals", "value": 6 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Side" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-text" },
              { "id": "mappings", "value": [
                { "type": "value", "options": { "BUY": { "color": "green", "text": "BUY" } } },
                { "type": "value", "options": { "SELL": { "color": "red", "text": "SELL" } } }
              ]}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "cellHeight": "sm",
        "footer": {
          "show": false,
          "reducer": ["sum"],
          "fields": ""
        },
        "sortBy": [
          { "displayName": "Time", "desc": true }
        ]
      }
    }
  ],
