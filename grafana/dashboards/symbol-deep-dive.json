    {
      "id": 5,
      "title": "Buy/Sell Ratio - ${symbol}USDT",
      "type": "timeseries",
      "datasource": {
        "type": "marcusolsson-json-datasource",
        "uid": "fastapi"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "refId": "A",
          "method": "GET",
          "urlPath": "/api/v1/analytics/klines/${symbol}USDT",
          "fields": [
            { "jsonPath": "$[*].timestamp", "name": "Time", "type": "time" },
            { "jsonPath": "$[*].buy_count", "name": "buy_count" },
            { "jsonPath": "$[*].sell_count", "name": "sell_count" },
            { "jsonPath": "$[*].trades_count", "name": "trades_count" }
          ]
        }
      ],
      "transformations": [
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "buy_count",
              "operator": "/",
              "right": "trades_count"
            },
            "alias": "buy_ratio"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "buy_ratio",
              "operator": "*",
              "right": "100"
            },
            "alias": "Buy %"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "sell_count",
              "operator": "/",
              "right": "trades_count"
            },
            "alias": "sell_ratio"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "sell_ratio",
              "operator": "*",
              "right": "100"
            },
            "alias": "Sell %"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "buy_count": true,
              "sell_count": true,
              "trades_count": true,
              "buy_ratio": true,
              "sell_ratio": true
            },
            "indexByName": {},
            "renameByName": {}
          }
        }
